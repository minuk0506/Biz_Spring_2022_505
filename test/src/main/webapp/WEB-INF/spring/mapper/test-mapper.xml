<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.minuk.test.persistance.TestDao">
	
	<select id="selectAll" resultType="TestDao">
		SELECT *
		FROM tbl_student
	</select>
	
	<select id="findById" resultType="TestDao">
		SELECT st_num
		FROM tbl_student
	</select>
	
	<select id="insert" resultType="TestDao">
		INSERT INTO tbl_student(
			st_num,
			st_name,
			st_grade,
			st_tel,
			st_addr
		) VALUE(
			#(st_num),
			#(st_name),
			#(st_grade),
			#(st_tel),
			#(st_addr)
		)
	</select>
</mapper> 